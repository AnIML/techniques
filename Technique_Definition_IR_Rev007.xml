<?xml version="1.0" encoding="UTF-8"?>
<Technique xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="..\schema\animl-technique.xsd" name="IR" extension="false" version="1.0">
	<Documentation>Technique definition for Infrared spectroscopy</Documentation>
	<SampleRole name="MeasurementSample" purpose="consumed" modality="required" maxOccurs="1" inheritable="true" upwardsInherited="false">
		<Documentation>This is the analyzed substance</Documentation>
		<ParameterCategoryBlueprint name="SampleDescription" modality="required">
			<Documentation>Parameters describing the measurement sample</Documentation>
			<ParameterBlueprint name="SampleNumber" type="int" modality="optional" maxOccurs="1">
				<Documentation>This parameter includes the sample number, it is just required when different sample numbers are required  </Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="SampleName" type="string" modality="required" maxOccurs="1">
				<Documentation>Here is the sample name, e.g. NIST Standard material A08E15.</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="CASName" type="string" modality="optional" maxOccurs="unbounded">
				<Documentation>CAS name of the sample.</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="Name" type="string" modality="optional" maxOccurs="unbounded">
				<Documentation>Other sample names (e.g. trade names)</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="SampleAmount" type="float" modality="optional" maxOccurs="1">
				<Documentation>Amount of sample used for measurement.</Documentation>
				<Unit label="g">
					<SIUnit exponent="1" factor="0.01" offset="0">kg</SIUnit>
				</Unit>
			</ParameterBlueprint>
			<ParameterBlueprint name="SampleConcentration" type="float" modality="optional" maxOccurs="1">
				<Documentation>Give Information about the Sample Concentration</Documentation>
				<Unit label="g/l">
					<SIUnit exponent="1" factor="0.01" offset="0">kg</SIUnit>
					<SIUnit exponent="-3" factor="0.001" offset="0">m</SIUnit>
				</Unit>
			</ParameterBlueprint>
			<ParameterBlueprint name="MolFormula" type="string" modality="optional" maxOccurs="unbounded">
				<Documentation>Totals Formula, e.g. C2H6O.</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="SampleState" type="string" modality="required" maxOccurs="1">
				<Documentation>Physical state/preparation of the sample.</Documentation>
				<AllowedValue>
					<string>Solid</string>
				</AllowedValue>
				<AllowedValue>
					<string>Liquid</string>
				</AllowedValue>
				<AllowedValue>
					<string>Gas</string>
				</AllowedValue>
				<AllowedValue>
					<string>Solution</string>
				</AllowedValue>
				<AllowedValue>
					<string>KBr Pellet</string>
				</AllowedValue>
				<AllowedValue>
					<string>Powder</string>
				</AllowedValue>
				<AllowedValue>
					<string>Nujol Mull</string>
				</AllowedValue>
			</ParameterBlueprint>
			<ParameterBlueprint name="CASRegistryNo" type="string" modality="optional" maxOccurs="unbounded">
				<Documentation>Sample's CAS registry number</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="BeilsteinLawsonNo" type="string" modality="optional" maxOccurs="unbounded">
				<Documentation>Sample's Beilstein-Lawson number</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="StereoSample" type="boolean" modality="optional" maxOccurs="1">
				<Documentation>Is this a stereo sample?</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="Disposal" type="string" modality="optional" maxOccurs="1">
				<Documentation>Instructions on safe disposal of sample</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="HandlingPrecautions" type="string" modality="optional" maxOccurs="1">
				<Documentation>Special instructions for handling the sample</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="Storage" type="string" modality="optional" maxOccurs="1">
				<Documentation>Instructions on safe storage of sample</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="UniformSampling" type="boolean" modality="optional" maxOccurs="1">
				<Documentation>"True" if uniform sampling used</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="AutoSamplerPosition" type="int" modality="optional" maxOccurs="1">
				<Documentation>Sample position in auto sampler device</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="SampleOwner" type="string" modality="optional" maxOccurs="1">
				<Documentation>Name of sample's owner</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="SampleReceipt" type="dateTime" modality="optional" maxOccurs="1">
				<Documentation>Date and time when sample was received</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="SamplingProcedureName" type="string" modality="optional" maxOccurs="1">
				<Documentation>Name/Description of sampling procedure used</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="SamplingPreparationProcedure" type="string" modality="optional" maxOccurs="1">
				<Documentation>Name/Description of sampling preparation  used</Documentation>
			</ParameterBlueprint>
			<ParameterCategoryBlueprint name="ChemicalStructure" maxOccurs="1" modality="optional">
				<Documentation>Description of the chemical structure</Documentation>
				<ParameterBlueprint name="Smiles" type="string" modality="optional" maxOccurs="unbounded">
					<Documentation>Chemical structure as SMILES string.</Documentation>
				</ParameterBlueprint>
				<ParameterBlueprint name="Wiswesser" type="string" modality="optional" maxOccurs="unbounded">
					<Documentation>Chemical structure as Wiswesser string.</Documentation>
				</ParameterBlueprint>
			</ParameterCategoryBlueprint>
			<ParameterCategoryBlueprint name="BP" maxOccurs="1" modality="optional">
				<Documentation>Boiling Point</Documentation>
				<ParameterBlueprint name="Min" type="float" modality="required" maxOccurs="1">
					<Documentation>Lower bound of boiling temperature range</Documentation>
					<Unit label="K">
						<SIUnit>K</SIUnit>
					</Unit>
					<Unit label="°C">
						<SIUnit offset="-273.15">K</SIUnit>
					</Unit>
					<Unit label="°F">
						<SIUnit offset="434.07" factor="1.8">K</SIUnit>
					</Unit>
				</ParameterBlueprint>
				<ParameterBlueprint name="Max" type="float" modality="optional" maxOccurs="1">
					<Documentation>Upper bound of boiling temperature range. Use only this value if sample has an actual boiling "point"</Documentation>
					<Unit label="K">
						<SIUnit>K</SIUnit>
					</Unit>
					<Unit label="°C">
						<SIUnit offset="-273.15">K</SIUnit>
					</Unit>
					<Unit label="°F">
						<SIUnit offset="434.07" factor="1.8">K</SIUnit>
					</Unit>
				</ParameterBlueprint>
				<ParameterBlueprint name="AtPressure" type="float" modality="required" maxOccurs="1">
					<Documentation>Pressure at which the boiling point has been determined.</Documentation>
					<Unit label="Pa">
						<SIUnit exponent="-1">m</SIUnit>
						<SIUnit exponent="1">kg</SIUnit>
						<SIUnit exponent="-2">s</SIUnit>
					</Unit>
				</ParameterBlueprint>
			</ParameterCategoryBlueprint>
			<ParameterCategoryBlueprint name="MP" maxOccurs="1" modality="optional">
				<Documentation>Melting Point</Documentation>
				<ParameterBlueprint name="Min" type="float" modality="required" maxOccurs="1">
					<Documentation>Lower bound of melting temperature range</Documentation>
					<Unit label="K">
						<SIUnit>K</SIUnit>
					</Unit>
					<Unit label="°C">
						<SIUnit offset="-273.15">K</SIUnit>
					</Unit>
					<Unit label="°F">
						<SIUnit offset="434.07" factor="1.8">K</SIUnit>
					</Unit>
				</ParameterBlueprint>
				<ParameterBlueprint name="Max" type="float" modality="optional" maxOccurs="1">
					<Documentation>Upper bound of melting temperature range. Use only this value if sample has an actual melting "point"</Documentation>
					<Unit label="K">
						<SIUnit>K</SIUnit>
					</Unit>
					<Unit label="°C">
						<SIUnit offset="-273.15">K</SIUnit>
					</Unit>
					<Unit label="°F">
						<SIUnit offset="434.07" factor="1.8">K</SIUnit>
					</Unit>
				</ParameterBlueprint>
				<ParameterBlueprint name="AtPressure" type="float" modality="required" maxOccurs="1">
					<Documentation>Pressure at which the melting point has been determined.</Documentation>
					<Unit label="Pa">
						<SIUnit exponent="-1">m</SIUnit>
						<SIUnit>kg</SIUnit>
						<SIUnit exponent="-2">s</SIUnit>
					</Unit>
				</ParameterBlueprint>
			</ParameterCategoryBlueprint>
			<ParameterCategoryBlueprint name="RefractiveIndex" maxOccurs="1" modality="optional">
				<Documentation>Refractive Index</Documentation>
				<ParameterBlueprint name="Value" type="float" modality="required" maxOccurs="1">
					<Documentation>Refractive index</Documentation>
				</ParameterBlueprint>
				<ParameterBlueprint name="AtTemperature" type="float" modality="required" maxOccurs="1">
					<Documentation>Temperature at which the refractive index has been determined.</Documentation>
					<Unit label="K">
						<SIUnit>K</SIUnit>
					</Unit>
					<Unit label="°C">
						<SIUnit offset="-273.15">K</SIUnit>
					</Unit>
					<Unit label="°F">
						<SIUnit offset="434.07" factor="1.8">K</SIUnit>
					</Unit>
				</ParameterBlueprint>
			</ParameterCategoryBlueprint>
		</ParameterCategoryBlueprint>
	</SampleRole>
