			</ParameterBlueprint>
			<ParameterBlueprint name="Preparer" parameterType="String" modality="optional" maxOccurs="unbounded">
				<Documentation>Person preparing the sample for analysis.</Documentation>
			</ParameterBlueprint>
			<ParameterBlueprint name="Amount Supplied" parameterType="Float" modality="optional" maxOccurs="1">
				<Documentation>Amount of material supplied (not all of which may have been been introduced).</Documentation>
				<Quantity name="Moles">
					<Unit label="mol">
						<SIUnit exponent="1">mol</SIUnit>
					</Unit>
					<Unit label="µmol">
						<SIUnit exponent="1">kg</SIUnit>
					</Unit>
					<Unit label="nmol">
						<SIUnit exponent="1">kg</SIUnit>
					</Unit>
					<Unit label="pmol">
						<SIUnit exponent="1">kg</SIUnit>
					</Unit>
				</Quantity>
				<Quantity name="Mass">
					<Unit label="g">
						<SIUnit exponent="1" factor="1e-3">kg</SIUnit>
					</Unit>
					<Unit label="mg">
						<SIUnit exponent="1" factor="1e-6">kg</SIUnit>
					</Unit>
					<Unit label="µg">
						<SIUnit exponent="1" factor="1e-9">kg</SIUnit>
					</Unit>
					<Unit label="ng">
						<SIUnit exponent="1" factor="1e-12">kg</SIUnit>
					</Unit>
				</Quantity>
			</ParameterBlueprint>
			<ParameterBlueprint name="Volume Supplied" parameterType="Float" modality="optional" maxOccurs="1">
				<Documentation>Volume of the supplied sample..</Documentation>
				<Quantity name="Volume">
					<Unit label="ml">
						<SIUnit exponent="3" factor="1e-6">m</SIUnit>
					</Unit>
					<Unit label="µl">
						<SIUnit exponent="3" factor="1e-9">m</SIUnit>
					</Unit>
				</Quantity>
			</ParameterBlueprint>
			<ParameterBlueprint name="Concentration" parameterType="Float" modality="optional" maxOccurs="1">
				<Documentation>Concentration of sample.</Documentation>
				<Quantity name="Concentration">
					<Unit label="g/ml">
						<SIUnit exponent="1" factor="1e-3">kg</SIUnit>
						<SIUnit exponent="-3" factor="1e-6">m</SIUnit>
					</Unit>
					<Unit label="mg/ml">
						<SIUnit exponent="1" factor="1e-6">kg</SIUnit>
						<SIUnit exponent="-3" factor="1e-6">m</SIUnit>
					</Unit>
					<Unit label="µg/ml">
						<SIUnit exponent="1" factor="1e-9">kg</SIUnit>
						<SIUnit exponent="-3" factor="1e-6">m</SIUnit>
					</Unit>
					<Unit label="ml/l">
						<SIUnit exponent="3" factor="1e-12">m</SIUnit>
						<SIUnit exponent="-3" factor="1e-3">m</SIUnit>
					</Unit>
					<Unit label="M">
						<SIUnit>mol</SIUnit>
						<SIUnit exponent="-3" factor="1e-6">m</SIUnit>
					</Unit>
					<Unit label="mM">
						<SIUnit exponent="1" factor="1e-3">kg</SIUnit>
						<SIUnit exponent="-3" factor="1e-6">m</SIUnit>
					</Unit>
					<Unit label="µM">
						<SIUnit exponent="1" factor="1e-6">kg</SIUnit>
						<SIUnit exponent="-3" factor="1e-6">m</SIUnit>
					</Unit>
					<Unit label="nM">
						<SIUnit exponent="1" factor="1e-9">kg</SIUnit>
						<SIUnit exponent="-3" factor="1e-6">m</SIUnit>
					</Unit>
					<Unit label="pM">
						<SIUnit exponent="1" factor="1e-12">kg</SIUnit>
						<SIUnit exponent="-3" factor="1e-6">m</SIUnit>
