				<Quantity name="Temperature">
					&degC; &degK; &degF;
				</Quantity>
			</ParameterBlueprint>
			<ParameterBlueprint name="Density" parameterType="Float" modality="optional" maxOccurs="1">
				<Documentation>Sample density as introduced. See Appl. Spectrosc. 42, 1, 1988.</Documentation>
				<Quantity name="Density">
					<Unit label="g/cm3">
