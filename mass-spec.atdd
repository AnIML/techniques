            <CategoryBlueprint name="Scan Law">
                <Documentation literatureReferenceID="psi-ms" literatureAccession="MS:1000019">
                    Description of the function of the mass scan law.
                </Documentation>
            </CategoryBlueprint>
        </CategoryBlueprint>
        <CategoryBlueprint name="Instrument Properties" modality="optional" maxOccurs="1">
            <Documentation>
                Fixed characteristics of the instrument.
            </Documentation>
            <ParameterBlueprint name="Vendor" parameterType="String" modality="optional">
