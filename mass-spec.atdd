                </ParameterBlueprint>
                <ParameterBlueprint name="Precursor Mass 2" parameterType="Numeric" maxOccurs="1" modality="optional">
                    <Quantity name="Mass/Charge Ratio">
                        &mz;
                    </Quantity>
                </ParameterBlueprint>
            </CategoryBlueprint>
            <CategoryBlueprint name="Full Scan Settings" maxOccurs="1" modality="optional">
                <CategoryBlueprint name="Mass Range" modality="optional" maxOccurs="1">
